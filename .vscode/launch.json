{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Simple Run",
			"program": "${workspaceRoot}/app.js",
			"args": [],
			"env": {
				"NODE_ENV": "development",
        "npm_package_config_contacts": "./contacts.json",
        "npm_package_config_port": "3232"
			}
		},
    {
			"type": "node",
			"request": "launch",
			"name": "List",
			"program": "${workspaceRoot}/app.js",
			"args": ["list"],
			"env": {
				"NODE_ENV": "development",
        "npm_package_config_contacts": "./contacts.json",
        "npm_package_config_port": "3232"
			}
		},
    {
			"type": "node",
			"request": "launch",
			"name": "Add Clark Kent",
			"program": "${workspaceRoot}/app.js",
			"args": ["--memory", "add", "Clark", "Kent"],
			"env": {
				"NODE_ENV": "development",
        "npm_package_config_contacts": "./contacts.json",
        "npm_package_config_port": "3232"
			}
		},
    {
			"type": "node",
			"request": "launch",
			"name": "Remove Bruce Wayne",
			"program": "${workspaceRoot}/app.js",
			"args": ["--memory", "remove", "EktuXZAKl"],
			"env": {
				"NODE_ENV": "development",
        "npm_package_config_contacts": "./contacts.json",
        "npm_package_config_port": "3232"
			}
		}
	]
}
